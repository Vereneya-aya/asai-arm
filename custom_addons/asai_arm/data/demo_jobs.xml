<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <!-- Готовы к работе -->
    <record id="asai_job_demo_1" model="asai.job">
        <field name="name">Заказ #1001 — раскрой полок</field>
        <field name="stage">cut</field>
        <field name="priority">1</field>
        <field name="status">ready</field>
    </record>
    <record id="asai_job_demo_2" model="asai.job">
        <field name="name">Заказ #1002 — кромка фасадов</field>
        <field name="stage">edge</field>
        <field name="priority">2</field>
        <field name="status">ready</field>
    </record>
    <record id="asai_job_demo_3" model="asai.job">
        <field name="name">Заказ #1003 — присадка деталей шкафа</field>
        <field name="stage">drill</field>
        <field name="priority">3</field>
        <field name="status">ready</field>
    </record>

    <!-- В работе -->
    <record id="asai_job_demo_4" model="asai.job">
        <field name="name">Заказ #1004 — присадка дверей</field>
        <field name="stage">drill</field>
        <field name="priority">2</field>
        <field name="status">in_progress</field>
        <field name="start_time">2025-09-02 09:00:00</field>
    </record>
    <record id="asai_job_demo_5" model="asai.job">
        <field name="name">Заказ #1005 — упаковка комплекта</field>
        <field name="stage">pack</field>
        <field name="priority">3</field>
