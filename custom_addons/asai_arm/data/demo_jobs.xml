        <field name="start_time">2025-09-02 10:00:00</field>
    </record>

    <!-- Готово -->
    <record id="asai_job_demo_6" model="asai.job">
        <field name="name">Заказ #1006 — раскрой столешницы</field>
        <field name="stage">cut</field>
        <field name="priority">1</field>
        <field name="status">done</field>
        <field name="start_time">2025-09-01 08:00:00</field>
        <field name="end_time">2025-09-01 12:00:00</field>
    </record>
    <record id="asai_job_demo_7" model="asai.job">
        <field name="name">Заказ #1007 — упаковка кухни</field>
        <field name="stage">pack</field>
        <field name="priority">2</field>
        <field name="status">done</field>
        <field name="start_time">2025-09-01 09:00:00</field>
        <field name="end_time">2025-09-01 15:00:00</field>
    </record>

    <!-- Брак -->
    <record id="asai_job_demo_8" model="asai.job">
        <field name="name">Заказ #1008 — кромка полки</field>
        <field name="stage">edge</field>
        <field name="priority">1</field>
        <field name="status">scrap</field>
    </record>
    <record id="asai_job_demo_9" model="asai.job">
        <field name="name">Заказ #1009 — присадка фасада</field>
        <field name="stage">drill</field>
        <field name="priority">2</field>
        <field name="status">scrap</field>
    </record>

    <!-- Невозможно выполнить -->
    <record id="asai_job_demo_10" model="asai.job">
        <field name="name">Заказ #1010 — раскрой нестандартной детали</field>
        <field name="stage">cut</field>
        <field name="priority">5</field>
        <field name="status">blocked</field>
    </record>
    <record id="asai_job_demo_11" model="asai.job">
        <field name="name">Заказ #1011 — упаковка нестандартного комплекта</field>
        <field name="stage">pack</field>
        <field name="priority">4</field>
        <field name="status">blocked</field>
    </record>

</odoo>